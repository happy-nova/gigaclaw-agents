{
  "$schema": "https://openclaw.ai/config.schema.json",
  "version": 1,
  
  "gateway": {
    "host": "0.0.0.0",
    "port": 18789,
    "token": "${OPENCLAW_TOKEN}"
  },

  "llm": {
    "provider": "anthropic",
    "model": "claude-sonnet-4-20250514",
    "apiKey": "${ANTHROPIC_API_KEY}"
  },

  "agents": {
    "default": "default",
    "list": [
      {
        "id": "default",
        "workspace": "./workspaces/default"
      }
    ]
  },

  "channels": {
    "slack": {
      "accounts": {}
    }
  },

  "bindings": []
}
